{
  "name": "ArcGISWebAppBuilder",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "start": "node server.js",
    "install-windows-service": "winser -i",
    "uninstall-windows-service": "winser -r",
    "test": "mocha --reporter spec --timeout 60s --recursive test/*/test*.js",
    "debug-test": "mocha --debug-brk --reporter spec --timeout 60s test/*/test*.js",
    "jenkins-test": "mocha --reporter mocha-jenkins-reporter --timeout 60s --recursive test/*/test*.js"
  },
  "devDependencies": {
    "mocha": "2.0.1",
    "mocha-jenkins-reporter": "0.1.2"
  },
  "dependencies": {
    "express": "4.2.0",
    "log4js": "0.6.14",
    "body-parser": "1.2.0",
    "errorhandler": "1.0.1",
    "ejs": "0.8.5",
    "tingodb": "0.3.0",
    "request": "2.33.0",
    "mongodb": "1.3.19",
    "fs-extra": "0.8.1",
    "deferred": "0.6.6",
    "jszip": "2.2.0",
    "pako": "0.1.1",
    "watch": "0.10.0",
    "compression": "1.0.8",
    "connect-multiparty": "1.0.5",
    "cookie-parser": "1.3.2",
    "requirejs": "2.1.14",
    "superagent": "0.20.0",
    "winser": "1.0.0"
  }
}
